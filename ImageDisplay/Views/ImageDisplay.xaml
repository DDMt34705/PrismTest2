<UserControl x:Class="ImageDisplay.Views.ImageDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="200" VerticalAlignment="Top" Width="200" Source="{Binding ImageSource}" Stretch="Fill"/>

    </Grid>
</UserControl>
